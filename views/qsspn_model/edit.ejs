				nbArrays=parseInt(Math.ceil(mlength/524288.0));
			}
			if (nbArrays==1){
				document.getElementById("layouts0").value=JSON.stringify(layoutJSON);
			}
			else{
				var stringSfba=JSON.stringify(layoutJSON);
				var chunksize=524288;
				var stringChunks=[];
				//console.log(mlength)
				for (var i=0;i<nbArrays;i++){
